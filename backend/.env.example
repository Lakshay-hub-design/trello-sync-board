# Trello API credentials
TRELLO_KEY=your_trello_key
TRELLO_TOKEN=your_trello_token

# Public webhook callback URL (must be Cloudflare Tunnel or Ngrok)
# Example:
# WEBHOOK_BASE_URL=https://your-tunnel-id.trycloudflare.com/api/webhook
# or
# WEBHOOK_BASE_URL=https://abc123.ngrok-free.app/api/webhook
WEBHOOK_BASE_URL=your_public_webhook_url

# Trello board ID
BOARD_ID=your_board_id

# Server port
PORT=4000
